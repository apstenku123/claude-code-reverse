# File: .env.example
# Purpose: Template for environment variables required by the project

# AI Service Configuration (Optional - only needed for AI features)
# These are used by the multi-model fix tool and AI integrations

# Google Gemini API
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-2.5-pro-preview-06-05

# Azure OpenAI Configuration
AZURE_OPENAI_KEY=your_azure_openai_key_here
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com
AZURE_API_VERSION=2024-12-01-preview

# Azure Model Deployments
AZURE_GPT4_1_DEPLOYMENT=gpt-4.1
AZURE_O4_MINI_DEPLOYMENT=o4-mini

# OpenAI Configuration (if using direct OpenAI)
OPENAI_API_KEY=your_openai_api_key_here

# Application Configuration
NODE_ENV=development
LOG_LEVEL=info

# Tool Configuration
MAX_CONCURRENT_REQUESTS=5
BATCH_SIZE=20
ENABLE_CHECKPOINTS=true